function addWindLayer(s, e) {
  if(!s){
    s = '2014-03-01';
  }
  if (!e) {
    e = '2014-03-27';
  }
  var ic = ee.ImageCollection('NASA/NLDAS/FORA0125_H002')
  //.filterDate('2017-03-01', '2017-03-27');
  .select(['wind_u', 'wind_v'])
  .filterDate(s, e);
  
  var layerWind = Map.addLayer(ee.Image(ic.first()), {min:0,max:10, /*palette: ['FF0000', '0000FF']*/}, 'wind', true, 0.40);
}

addWindLayer()