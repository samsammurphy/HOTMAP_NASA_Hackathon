/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var imageCollection = ee.ImageCollection("NASA/GPM_L3/IMERG");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var ic = ee.ImageCollection('UCSB-CHG/CHIRPS/PENTAD')
.filterDate('2017-03-01','2017-03-27')

var meanPrecip = ic.reduce(ee.Reducer.mean())
  
Map.addLayer(meanPrecip, {min:0,max:10, palette: ['FF0000', '0000FF']}, 'precipitation', true, 0.40)
  
