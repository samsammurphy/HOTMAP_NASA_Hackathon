/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var point = /* color: #d63000 */ee.Geometry.Point([-122.262, 37.8719]);
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var start = ee.Date('2013-06-01');
var finish = ee.Date('2016-10-01');

var v = ee.ImageCollection('LANDSAT/LC8_L1T_TOA')
  .filterBounds(point).filterDate(start, finish)
  .select(['B4', 'B3', 'B2'])
  .map(function(image) {
    return image.multiply(512).uint8();//.visualize()
  });

Export.video.toDrive(v);