/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var point = /* color: #d63000 */ee.Geometry.Point([-122.262, 37.8719]);
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var start = ee.Date('2014-06-01');
var finish = ee.Date('2014-10-01');

var v = ee.ImageCollection('LANDSAT/LT5_L1T_TOA')
  .filterBounds(point).filterDate(start, finish).select(['B4', 'B3', 'B2']);

Export.video.toCloudStorage(v);